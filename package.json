    "prisma": "^4.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.4.3",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "typed.js": "^2.0.16",
    "yup": "^1.1.1"
